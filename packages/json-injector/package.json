{
  "name": "json-injector",
  "description": "Inject variables from environment variables or js/json files to json",
  "version": "0.2.0",
  "main": "lib/app.js",
  "repository": "https://github.com/kimxogus/json-injector.git",
  "author": "kimxogus <kgyoo8232@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "test": "jest",
    "clean": "rm -rf lib",
    "prebuild": "yarn clean",
    "build": "babel ./src --out-dir ./lib --copy-files",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "ajv": "^5.2.3",
    "ajv-keywords": "^2.1.0",
    "fs-extra": "^4.0.2",
    "glob": "^7.1.2",
    "inject-env": "^0.6.1",
    "lodash": "^4.17.4"
  },
  "files": [
    "lib",
    "README.md",
    "LICENSE"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "testEnvironment": "node",
    "transform": {
      "^.+\\.js?$": "babel-jest"
    },
    "testPathIgnorePatterns": [
      "[/\\\\](build|docs|node_modules|scripts|example)[/\\\\]"
    ],
    "watchPathIgnorePatterns": [
      "testConfig/.*.json",
      "lib"
    ]
  }
}
