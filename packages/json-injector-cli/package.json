{
  "name": "json-injector-cli",
  "version": "0.1.0",
  "repository": "https://github.com/kimxogus/json-injector.git",
  "author": "kimxogus <kgyoo8232@gmail.com>",
  "license": "MIT",
  "private": false,
  "bin": {
    "json-injector": "lib/app.js"
  },
  "scripts": {
    "test": "jest",
    "clean": "rm -rf lib",
    "prebuild": "yarn clean",
    "build": "babel ./src --out-dir ./lib --copy-files",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "yargs": "^9.0.1"
  },
  "peerDependencies": {
    "json-injector": "^0.1.0 || ^0.2.0"
  },
  "files": ["lib", "README.md", "LICENSE"],
  "jest": {
    "moduleFileExtensions": ["js", "json"],
    "testEnvironment": "node",
    "transform": {
      "^.+\\.js?$": "babel-jest"
    },
    "testPathIgnorePatterns": [
      "[/\\\\](build|docs|node_modules|scripts|example)[/\\\\]"
    ],
    "watchPathIgnorePatterns": ["testConfig/.*.json", "lib"]
  }
}
